<Window x:Class="WebDays2022.WPF.InMemory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebDays2022.WPF.InMemory"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Feature status" Click="ButtonPage1_Click" MinWidth="100"/>
            <Button Content="Feature settings" Click="ButtonPage2_Click" MinWidth="100" Margin="10,0,0,0"/>
        </StackPanel>
        <Frame Name="Main" Margin="0,35,0,0" />
    </Grid>
</Window>
