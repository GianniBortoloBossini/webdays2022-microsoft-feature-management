<Page x:Class="WebDays2022.WPF.InMemory.FeaturesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WebDays2022.WPF.InMemory"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Features page">

    <StackPanel>
        <RadioButton x:Name="featureEnabled" Content = "Enabled"  
         GroupName = "Feature" HorizontalAlignment = "Left" 
         VerticalAlignment = "Top" Width = "95" Checked = "FeatureEnabled_Check" />
        <RadioButton x:Name = "featureDisabled" Content = "Disabled"  
         GroupName = "Feature" HorizontalAlignment = "Left"
         VerticalAlignment = "Top" Width = "95" Checked = "FeatureDisabled_Check" />
    </StackPanel>
</Page>
